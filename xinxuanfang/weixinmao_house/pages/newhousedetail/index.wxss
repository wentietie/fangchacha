@import '../index/css';
@import '../sidan/commentlist';
page {
    height: 100%;
    background-color: #e5e5e5;
}
view[hidden] {
  display: none !important;
}

.scr {
    position: relative;
    height: 100%;
}


.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-radius: 50%;
    transition: all .6s;
    opacity: .5;
}

.swiper-container .dots .dot.active {
    width: 14rpx;
    opacity: 1;
}

.container .header {
    display: flex;
    width: 750rpx;
    flex-direction: column;
    align-items: center;
    background: #FFF;
}

.container .header>image {
    width: 750rpx;
    height: 350rpx;
}

.container .main {
    display: flex;
    width: 750rpx;
    flex-direction: column;
    padding-bottom: 20rpx;
    background: #FFF;
}
.container .plat {
    background: #FFF;
}

.container .main .title {
    display: flex;
    width: 700rpx;
    flex-direction: column;
    padding-top: 20rpx;
    font-size: .9rem;
    padding-left: 30rpx;
}

.container .main .title view {
}

.container .main .title view:nth-child(1) {
    font-size: 1.1rem;
}

.container .main .title .price {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 700rpx;
}

.container .main .title .price view:nth-child(1) {
    font-size: .9rem;
    color: #d51a39;
}

.container .main .title .price view:nth-child(2) {
    font-size: .8rem;
    color: #FFF;
    background: #ff706a;
    border-radius: 3rpx;
    padding: 5rpx 20rpx;
}

.container .main .title .price view:nth-child(3) {
    width: 190rpx;
}

.container .main .title .price button {
    width: 190rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #1aac19;
    font-size: .6rem;
    border-radius: 10rpx;
}

.container .main .address {
    display: flex;
    flex-direction: column;
    padding-left: 30rpx;
    font-size: .7rem;
    margin-top: 10rpx;
    color: #777676;
}

.container .main .special,.container .plat .special {
    width: auto;
    display: flex;
    flex-direction: row;
    padding-left: 30rpx;
    margin-top: 20rpx;
    flex-wrap: wrap;
}
.container .plat .special {
    margin-top: 0;
    padding-left: 150rpx
}
.container .main .special text,.container .plat .special text{
    padding: 5rpx 10rpx;
    border-radius: 5rpx;
    font-size: .6rem;
    background: red;
    margin-right: 10rpx;
    color: #FFF;
    margin-bottom: 10rpx;
}

.container .plat .special text{
  font-size: .8rem;
  margin-right: 30rpx;
}
.container .main .special text:nth-child(2n),.container .plat .special text:nth-child(2n) {
    background: #ecf2fe;
    color: #90a1cf;
}

.container .main .special text:nth-child(2n-1),.container .plat .special text:nth-child(2n-1)  {
    background: #e3f9e4;
    color: #7dac82;
}

.container .contentlist {
    margin-top: 10rpx;
    width: 750rpx;
    background: #FFF;
    display: flex;
    flex-direction: column;
}

.container .contentlist .title {
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: .9rem;
    border-bottom: 1rpx solid #efefef;
    display: flex;
    flex-direction: row;
    align-content: center;
}

.container .contentlist.line .title view:nth-child(1) {
    height: 40rpx;
    line-height: 40rpx;
    width: 6rpx;
    background: #3274e5;
    align-self: center;
    padding-right: 0;
    padding-left: 0;
}


.container .contentlist .title view{
    height: 80rpx;
    line-height: 80rpx;
    width: auto;
    padding-left: 10rpx;
    padding-right: 10rpx;
    margin-right: 10rpx;
    font-size: .9em;
}
.container .contentlist .title view.active{
   color: red;
   border-bottom: 2rpx solid red;
   box-sizing: border-box;
}

.container .contentlist .listitem {
    width: 750rpx;
    display: flex;
    flex-direction: column;
}

.container .contentlist .listitem .items {
    padding-left: 30rpx;
    color: #777676;
    padding-top: 15rpx;
    padding-bottom: 15rpx;
    font-size: .8rem;
    border-bottom: 1rpx dotted #e5e5e5;
}

.container .contentlist .listitem .items text {
    color: #000;
    padding-left: 10rpx;
}

.type-container {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.type-navbar {
    background-color: #fff;
    width:100%;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.type-box {
    padding-top: 10rpx;
    width: 250rpx;
    box-sizing: content-box;
    font-size: 24rpx;
    text-align: center;
    display: inline-block;
    overflow: hidden;
    padding-left: 10rpx;
}

.type-box image {
    width: 230rpx;
    height: 230rpx;
}

.type-navbar-item {
    border-bottom: 4rpx solid #fff;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.type-item-on {
    border-bottom: 4rpx solid #e64340;
}

.agentinfo {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    background: #FFF;
    margin-top: 10rpx;
    justify-content: space-between;
}

.agentinfo .left {
    width: auto;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    padding-top: 20rpx;
}

.agentinfo .left .headpic image {
    width: 120rpx;
    height: 120rpx;
    border: 7rpx solid #FFF;
    border-radius: 120rpx;
}

.agentinfo .left .username {
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
}

.agentinfo .left .username view:nth-child(1) {
    font-size: .8rem;
    height: 40rpx;
    line-height: 40rpx;
    color: unset;
}

.agentinfo .left .username view {
    font-size: .7rem;
    height: 50rpx;
    line-height: 50rpx;
    color: #777676;
}

.agentinfo .right {
    width: 200rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.agentinfo .right .contact {
    display: flex;
    flex-direction: column;
    padding-top: 20rpx;
}

.agentinfo .right .contact view:nth-child(1) {
    font-size: .8rem;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
}

.agentinfo .right .contact view:nth-child(2) {
    font-size: .7rem;
    height: 60rpx;
    line-height: 60rpx;
    color: #777676;
}

.agentinfo .right .contact text {
    font-size: 1.5rem;
    color: #44b549;
}


.weixinmao-float {
    height: 100rpx;
}

.weixinmao-foot {
    display: flex;
    justify-content: center;
    flex-direction: row;
    position: fixed;
    z-index: 99999;
    left: 0;
    bottom: 0;
    width: 100%;
    border-top: 1rpx solid #ecf2fe;
}

.weixinmao-foot {
    align-self: center;
    height: 100rpx;
    display: flex;
    flex-direction: row;
    border-radius: 0rpx;
    background: #FFF;
}

.weixinmao-foot .footer {
    font-size: .7rem;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #777676;
    height: 100%;
}

.weixinmao-foot view:nth-child(1) {
    width: 150rpx;
}

.weixinmao-foot view:nth-child(2) {
    width: 150rpx;
}

.weixinmao-foot view:nth-child(3) {
    width: 150rpx;
}

.weixinmao-foot view:nth-child(4) {
    width: 400rpx;
    background: #44b549;
    color: #fff;
    line-height: 100rpx;
    margin-top: 0rpx;
    font-size: .8rem;
}

.weixinmao-foot .footer text {
    font-size: 1.8rem;
    color: #44b549;
}

.weixinmao-foot .footer text:nth-child(1) {
    font-size: 1.5rem;
}

.weixinmao-foot .footer text:nth-child(3) {
    font-size: 1.5rem;
}

.weixinmao-foot .footer view {
    width: 150rpx;
    text-align: center;
}

.intro {
    align-self: center;
    padding-left: 2%;
    padding-right: 2%;
}

.news {
    margin-top: 10rpx;
    width: 750rpx;
    background: #FFF;
    display: flex;
    flex-direction: column;
}

.news .newstitle {
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: .8rem;
    border-bottom: 1rpx solid #efefef;
    margin: 0 auto;
    display: flex;
    flex-direction: row;
    align-content: center;
}

.news .newstitle view:nth-child(1) {
    height: 40rpx;
    line-height: 40rpx;
    width: 6rpx;
    background: #3274e5;
    align-self: center;
}

.news .newstitle view:nth-child(2) {
    height: 80rpx;
    line-height: 80rpx;
    width: auto;
    padding-left: 15rpx;
}

.news .newslist {
    display: flex;
    flex-direction: row;
    width: 720rpx;
    margin: 0 auto;
    border-bottom: 1px solid #efefef;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
}

.news .newslist .pic {
    width: 35%;
    height: 220rpx;
    line-height: 220rpx;
    align-items: center;
    justify-content: center;
    display: flex;
}

.news .newslist .pic image {
    width: 260rpx;
    height: 190rpx;
}

.news .newslist .title {
    width: 65%;
    flex-direction: column;
    display: flex;
    justify-content: space-between;
    height: 190rpx;
    padding-top: 25rpx;
}

.news .newslist .title .address {
    padding-left: 5%;
    text-align: left;
    font-size: .7rem;
    color: #000;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    height: 110rpx;
}

.news .newslist .title .address view:nth-child(1) {
    height: 30rpx;
    line-height: 30rpx;
    font-size: .8rem;
}

.news .newslist .title .address view:nth-child(2) {
    height: 30rpx;
    line-height: 30rpx;
    color: #979797;
}

.news .newslist .title .address view:nth-child(3) {
    height: 30rpx;
    line-height: 30rpx;
    color: #979797;
}

.news .newslist .title .price {
    padding-left: 5%;
    text-align: left;
    font-size: .7rem;
    color: #6a6a6a;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 60rpx;
    line-height: 60rpx;
    align-self: baseline;
    width: 450rpx;
}

.news .newslist .title .price view text {
    padding: 10rpx 15rpx;
    border-radius: 3rpx;
    margin-right: 5rpx;
    height: 20rpx;
    line-height: 20rpx;
    color: #77af6e;
    background: #f7f5f6;
}

.news .newslist .title .price .money {
    color: #d65408;
    font-weight: bold;
    font-size: 1rem;
    padding-right: 10rpx;
}

.weixinmao-msg-float {
    height: 180rpx;
}

.weixinmao-msg-foot {
    display: flex;
    justify-content: center;
    flex-direction: row;
    position: fixed;
    z-index: 99999;
    left: 0;
    bottom: 0;
    width: 100%;
    border-top: 1rpx solid #ecf2fe;
    background: #FFF;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.weixinmao-msg-foot input {
    width: 500rpx;
    border-radius: 10rpx;
    border: 1rpx solid #e5e5e5;
    height: 80rpx;
    margin-left: 60rpx;
}

.weixinmao-msg-foot button {
    height: 80rpx;
    line-height: 80rpx;
    font-size: .8rem;
    color: #999999;
    background: #fff;
}

button::after {
    border: none;
}

.weixinmao-msg_show {
    position: fixed;
    top: 0%;
    left: 0%;
    width: 100%;
    height: 100%;
    background-color: black;
    z-index: 2001;
    background: rgba(0,0,0,0.7);
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.weixinmao-content_show {
    width: 550rpx;
    height: 600rpx;
    z-index: 4001;
    background: #FFF;
    align-self: center;
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.weixinmao-content_show .title {
    border-bottom: 1rpx solid #e5e5e5;
    width: 100%;
    text-align: center;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    font-size: .8rem;
    background: #3274e5;
    color: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.weixinmao-content_show .title view:nth-child(1) {
    width: 30%;
}

.weixinmao-content_show .title view:nth-child(2) {
    width: 40%;
}

.weixinmao-content_show .title view:nth-child(3) {
    width: 30%;
}

.weixinmao-content_show .list {
    display: flex;
    flex-direction: column;
    width: 100%;
    align-items: center;
}

.weixinmao-content_show .list .item {
    width: 100%;
    align-self: center;
    text-align: center;
    padding-top: 15rpx;
    padding-bottom: 15rpx;
    border-bottom: 1rpx solid #f5f5f5;
}
.btn-icon{
  padding: 30rpx 0;
  text-align: center;
  font-size: .7em;
}
.btn-icon .btn-icon-item image{
  width: 100rpx;
  height: 100rpx;
}
button.not-btn,button.not-btn:hover{
  line-height: inherit;
  font-size:inherit;
  background-color: inherit;
  color: inherit;
  padding-left: inherit;
  padding-right: inherit;
  border-radius: inherit;  
}
.btn-jion{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  box-shadow: 0px 4rpx 8rpx rgba(0,0,0,0.3);
  background-color: #fff;
  box-sizing: border-box;
  text-align: center;
  font-size: small;
}
.btn-jion .btns button{
  border: none;
  margin:30rpx;
  color: #fff;
  font-size: .77rem;
  line-height: 3;
}
.btn-jion .btns  button:first-child{
  margin-right: 0;
  margin-left:15rpx; 
}
.btn-jion  .btns button:nth-child(2){
  background-color:#3274e5; 
}
.btn-video{
  top:10rpx;
  right: 20rpx;
  text-align: center;
  font-size: .7rem;
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  background-color:rgba(0, 0, 0, 0.55);
  border-radius: 50%;
  box-sizing: border-box;
  padding-top: 8rpx;
  color: #fff;
  vertical-align: middle;
  display: table
}

.btn-video view{
  vertical-align: middle;
  display: table-cell;
}