

<form reportSubmit bindsubmit="savepubinfo">
    <view class="container">
        <view class="main">
            <view class="list">
                <view class="item">
                    <view class="name">发布公司</view>
                    <view class="selectitem">
                        {{item.cname}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">发布公司区域</view>
                    <view class="selectitem">
                        {{item.cname_q}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">姓 名</view>
                    <view class="selectitem">
                        {{item.name}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">联系方式</view>
                    <view class="selectitem" style="font-weight: bold;">
                        {{item.tel}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">私单员工</view>
                    <view class="selectitem">
                        {{item.s_name}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">私单公司</view>
                    <view class="selectitem">
                        {{item.s_cname}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">走私佣金总计</view>
                    <view class="selectitem">
                        {{item.yongjin}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">单量</view>
                    <view class="selectitem">
                        {{item.total}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">成交时间</view>
                    <view class="selectitem">
                        {{item.times}}
                    </view>
                </view>
                <view class="item">
                    <view class="name">备 注:</view>
                </view>
                <view class="item">
                    <view class="selectitem" style="width:auto">
                        {{item.desc}}
                    </view>
                </view>             
            </view>
        </view>
    </view>
</form>
 
<include src="commentlist.wxml" />
<include src="sidan.wxml" />

<view class="u-tap-btn">
    <button class="u-go-home" open-type="share">
        <text class="iconfont fa fa-share-alt"/> 分享
    </button>
    <button class="u-go-home" bindtap="toUrl" data-url="tab:../index/index">
        <text class="iconfont fa fa-home"/> 首页
    </button>
    <button class="u-go-home" openType="contact">
        <text class="iconfont fa fa-comments-o"/> 客服
    </button>
</view>