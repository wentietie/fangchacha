swiper .wx-swiper-dot {
	width: 14rpx;
	height: 14rpx;
	position: relative;
	background-color: rgba(255, 255, 255, .3);
	margin: 4rpx 8rpx !important;
}

swiper .wx-swiper-dot.wx-swiper-dot-active::after {
	content: "";
	position: absolute;
	width: 14rpx;
	height: 14rpx;
	top: 0rpx;
	left: 0rpx;
	right: 0;
	bottom: 0;
	margin: auto;
	background-color: #fff;
	border-radius: 20rpx;
}

