.plat-box{
  padding: 30rpx 10rpx;
  min-height: 330rpx;
  box-sizing: border-box;
}
.plat-box .plat-item{
   height: 100%;
   background: linear-gradient(-45deg, #f4d194, #fee7b3 50%, #ffda86);
   margin: 0 10rpx;
   border-radius: 15rpx;
   text-align: center;
   font-size: 30rpx;
   box-sizing: border-box;
   padding-bottom: 30rpx;
}
.plat-box .plat-item .avatar{
  height: 125rpx;
  width: 125rpx;
  border-radius: 50%;
  border: 1px #e4daab solid;
  margin-top: 20rpx;
}
.plat-box .plat-item .name{
  font-weight: bold;color: #986b1b;
}
.plat-box .plat-item .desc{
  font-size: .9em;color: #b69f76;
}
.plat-box .plat-item .name,.plat-box .plat-item .desc{
  overflow: hidden;
  padding: 0 10rpx;
  height: 45rpx;
  white-space:nowrap;
  max-width: 99%;
}
.plat-box .plat-item .btn{
  border-radius: 40rpx;
  font-size: 30rpx;
  margin: 5rpx 30rpx;
  padding: 0 5rpx;
  line-height: 2;
  background-color: #f9f9f1;
  color: #986b1b;
  padding-left: 5rpx;
  padding-right: 5rpx;
  box-shadow: 0px 4rpx 8rpx rgba(0,0,0,0.15);
}
.o-hide{
  overflow: hidden;
  white-space:nowrap;
}
.plat-list{
  background: linear-gradient(-45deg, #f4d194, #fee7b3 50%, #ffda86);
  border-radius: 15rpx;
  margin: 20rpx;
  font-size: 30rpx;
  padding-top:40rpx; 
  color: #49422e;
  overflow: hidden;
}
.plat-list .name{
  font-weight: bolder;
  font-size:large;
}
.plat-list .avatar{
  border-radius: 50%;
  height: 125rpx;
  width: 125rpx;
  border: 1px #fff solid;
  margin: 0 30rpx;
}
.plat-list .bottom{
  margin: 0 20rpx;
  border-top: 1px #fff solid;
  padding: 20rpx 0;
  font-size: .8em;
  text-align: center;
}

.plat-list .bottom .num{
  font-weight:bolder;
}
.plat-list .btn{
  border-radius: 40rpx;
  font-size: 30rpx;
  margin: 30rpx 30rpx 30rpx 0;
  padding: 0;
  line-height: unset;
  padding-left: 5rpx;
  padding-right: 20rpx;
  box-shadow: 0px 4rpx 8rpx rgba(0,0,0,0.15);
  background-color:#fdd272; 
  color: #49422e;
  box-sizing: content-box;
  width: 165rpx;
  border: none;
  float: left;
  white-space: nowrap;
}
.plat-list .btn::after{  
  border: none;
}
.plat-list .btn .fa.large{
  font-size: large;
}
.plat-list .btn .fa{
  font-size: .9rem;
  background-color: #fff;
  width: 35rpx;
  height: 35rpx;
  border-radius: 50%;
  padding: 10rpx;
  text-align: center;
  margin: 5rpx 15rpx 5rpx 0;
}
.plat-list .btn .text{  
  line-height: 50rpx;
  font-size: .8em;
  margin-top: 10rpx;
}

